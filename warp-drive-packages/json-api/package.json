{
  "name": "@warp-drive/json-api",
  "version": "5.9.0-alpha.8",
  "description": "A {json:api} Cache Implementation for WarpDrive",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "Chris Thoburn <runspired@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:warp-drive-data/warp-drive.git",
    "directory": "warp-drive-packages/json-api"
  },
  "scripts": {
    "build:pkg": "vite build",
    "prepack": "pnpm run build:pkg",
    "lint": "eslint . --quiet --cache --cache-strategy=content",
    "sync": "echo \"syncing\"",
    "start": "vite"
  },
  "files": [
    "dist",
    "declarations",
    "addon-main.cjs",
    "CHANGELOG.md",
    "README.md",
    "LICENSE.md",
    "logos"
  ],
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "peerDependencies": {
    "@warp-drive/core": "workspace:*"
  },
  "dependencies": {
    "@embroider/macros": "^1.19.6",
    "json-to-ast": "2.1.0",
    "fuse.js": "7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@types/json-to-ast": "^2.1.4",
    "@warp-drive/internal-config": "workspace:*",
    "@warp-drive/core": "workspace:*",
    "decorator-transforms": "^2.3.0",
    "expect-type": "^1.2.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "ember": {
    "edition": "octane"
  }
}
